{
  "metadata": [
    {
      "src": [
        {
          "cwd": "../MyMath/Math.Lib",
          "files": [ "Math.Lib.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ]
        },
        {
          "cwd": "../MyMath/Math.Tests",
          "files": [ "Math.Tests.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ]
        }
      ],
      "dest": "api",
      "properties": {
        "GenerateDocumentationFile": true
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.md" ]
      },
      {
        "files": [ "api/**.yml" ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "MyMath Library",
      "_enableSearch": true
    }
  }
}
